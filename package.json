{
    "name": "lzyun",
    "version": "1.0.0",
    "description": "OM",
    "author": "liugenyuan@zhihuijianshe.com",
    "private": true,
    "scripts": {
        "dev": "cross-env BABEL_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --host 0.0.0.0",
        "start": "npm run dev",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "test": "npm run unit",
        "lint": "eslint --ext .js,.vue src test/unit",
        "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
        "prettier": "prettier --write ./src/**/*.vue",
        "build:sit": "cross-env NODE_ENV=sit node --max_old_space_size=2048 build/build.js",
        "build:deploy": "cross-env NODE_ENV=production node --max_old_space_size=4096 build/build.js",
        "build:develop": "cross-env NODE_ENV=develop node --max_old_space_size=2048 build/build.js",
        "build:uat": "cross-env NODE_ENV=uat node --max_old_space_size=2048 build/build.js",
        "svg": "vsvg -s ./src/assets/svg -t ./src/views/icons",
        "push": "node ./build/push.js"
    },
